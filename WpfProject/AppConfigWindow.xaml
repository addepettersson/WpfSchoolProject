<Window x:Class="WpfProject.AppConfigWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:WpfProject"
        mc:Ignorable="d"
        Title="AppConfigWindow" Height="300" Width="300">
    <StackPanel>
        <Label Content="Skriv in adressen till API:et" HorizontalAlignment="Center"/>
        <TextBox x:Name="txtNewApiString" Width="100"/>
        <Button x:Name="btnCreateNewApi" Content="Ändra APi adressen" Click="btnCreateNewApi_Click"/>
    </StackPanel>
</Window>
